{
  "family": "task-byx-roteador-contrato-staging",
  "containerDefinitions": [
    {
      "name": "container-roteador-contrato-staging",
      "image": "256139846389.dkr.ecr.us-east-1.amazonaws.com/byx-roteador-contrato-staging:latest",
      "portMappings": [
        {
          "name": "container-roteador-contrato-staging-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NEW_RELIC_APP_NAME",
          "value": "byx-roteador-contrato-staging"

        },
        {
          "name": "CNAME",
          "value": "roteador-contrato"
        }
      ],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::byxenvs/roteador-contrato/staging/.env",
          "type": "s3"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/task-byx-roteador-contrato-staging",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "taskRoleArn": "arn:aws:iam::256139846389:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::256139846389:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
